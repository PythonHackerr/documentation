{
  "rewrites": [
    { "source": "/documentation/:asset*", "destination": "/:asset*" }
  ],
  "redirects": [
    { "source": "/", "destination": "/documentation/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "trailingSlash": false
}